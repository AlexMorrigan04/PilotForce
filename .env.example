# AWS Credentials - Replace with your actual credentials
# For production, use Cognito Identity Pools or STS instead of hard-coded credentials
REACT_APP_AWS_ACCESS_KEY_ID=your_access_key_here
REACT_APP_AWS_SECRET_ACCESS_KEY=your_secret_key_here
REACT_APP_AWS_REGION=your_region_here

# API Keys
REACT_APP_MAPBOX_ACCESS_TOKEN=your_mapbox_token_here
REACT_APP_WEBODM_API_KEY=your_webodm_api_key_here
REACT_APP_RESEND_API_KEY=your_resend_api_key_here

# Google Maps
REACT_APP_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Cognito Configuration
REACT_APP_USER_POOL_ID=your_user_pool_id_here
REACT_APP_USER_POOL_WEB_CLIENT_ID=your_web_client_id_here

# Security Options
REACT_APP_ENFORCE_HTTPS=true
REACT_APP_COGNITO_ADVANCED_SECURITY=true
REACT_APP_SESSION_TIMEOUT_MINUTES=60
